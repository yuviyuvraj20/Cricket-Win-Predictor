<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>prediction</servlet-name>
        <servlet-class>CricPrediction.prediction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>com.machinelearn.indiaprediction.admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RandomForestPredictor</servlet-name>
        <servlet-class>com.machinelearn.indiaprediction.RandomForestPredictor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>datasetup</servlet-name>
        <servlet-class>com.machinelearn.indiaprediction.datasetup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>datat</servlet-name>
        <servlet-class>com.machinelearn.indiaprediction.datat</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>prediction</servlet-name>
        <url-pattern>/prediction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RandomForestPredictor</servlet-name>
        <url-pattern>/RandomForestPredictor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>datasetup</servlet-name>
        <url-pattern>/datasetup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>datat</servlet-name>
        <url-pattern>/datat</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
